// Program07.java
// BankAccount class with accountNumber, balance and class variable bankName
public class Program07 {
    String accountNumber;
    double balance;
    static String bankName = "State Bank";

    public Program07(String accountNumber, double balance) {
        this.accountNumber = accountNumber;
        this.balance = balance;
    }

    // deposit method using a local variable amount
    public void deposit(double amt) {
        double amount = amt; // local variable
        balance += amount;
        System.out.println("Deposited " + amount + ", New balance = " + balance);
    }

    public void display() {
        System.out.println("Account: " + accountNumber + ", Balance: " + balance + ", Bank: " + bankName);
    }

    public static void main(String[] args) {
        String acc = "SBIN0001";
        double bal = 2500.0;
        Program07 a = new Program07(acc, bal);

        a.display();
        a.deposit(1500.0); // deposit using local variable inside method
    }
}
