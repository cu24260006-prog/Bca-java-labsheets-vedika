// Program11.java
// Calculator class with add, subtract, multiply, divide and operationsCount
public class Program11 {
    static int operationsCount = 0; // class variable

    public double add(double x, double y) {
        double result = x + y; // local
        operationsCount++;
        return result;
    }

    public double subtract(double x, double y) {
        double result = x - y;
        operationsCount++;
        return result;
    }

    public double multiply(double x, double y) {
        double result = x * y;
        operationsCount++;
        return result;
    }

    public double divide(double x, double y) {
        double result;
        if (y == 0) result = Double.NaN;
        else result = x / y;
        operationsCount++;
        return result;
    }

    public static void main(String[] args) {
        // local variables
        double a = 20.0, b = 5.0;
        Program11 calc = new Program11();

        System.out.println("Add: " + calc.add(a, b));
        System.out.println("Subtract: " + calc.subtract(a, b));
        System.out.println("Multiply: " + calc.multiply(a, b));
        System.out.println("Divide: " + calc.divide(a, b));
        System.out.println("Total operations = " + Program11.operationsCount);
    }
}
// Program12.java
// StudentMarks: calculate average with universityName class variable
public class Program12 {
    double marks1, marks2, marks3;
    static String universityName = "State University";

    public Program12(double m1, double m2, double m3) {
        this.marks1 = m1;
        this.marks2 = m2;
        this.marks3 = m3;
    }

    public double average() {
        double avg = (marks1 + marks2 + marks3) / 3.0; // local
        return avg;
    }

    public static void main(String[] args) {
        double m1 = 70, m2 = 65, m3 = 80;
        Program12 s = new Program12(m1, m2, m3);

        System.out.println("University: " + Program12.universityName);
        System.out.println("Average = " + s.average());
    }
}
// Program13.java
// Temperature: convert celsius to Fahrenheit and Kelvin
public class Program13 {
    double celsius;
    static String scale = "Celsius";

    public Program13(double celsius) {
        this.celsius = celsius;
    }

    public double toFahrenheit() {
        double f = (celsius * 9.0 / 5.0) + 32.0; // local
        return f;
    }

    public double toKelvin() {
        double k = celsius + 273.15; // local
        return k;
    }

    public static void main(String[] args) {
        double c = 25.0;
        Program13 t = new Program13(c);

        System.out.println("Scale = " + Program13.scale);
        System.out.println("Celsius = " + c);
        System.out.println("Fahrenheit = " + t.toFahrenheit());
        System.out.println("Kelvin = " + t.toKelvin());
    }
}
// Program14.java
// Person: check voting eligibility (country static)
public class Program14 {
    String name;
    int age;
    static String country = "India";

    public Program14(String name, int age) {
        this.name = name;
        this.age = age;
