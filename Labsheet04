    }
}
// Program15.java
// SimpleInterest: calculate interest using local variables
public class Program15 {
    double principal;
    double rate;
    double time;
    static String bank = "City Bank";

    public Program15(double p, double r, double t) {
        this.principal = p;
        this.rate = r;
        this.time = t;
    }

    public double calculateInterest() {
        double interest = (principal * rate * time) / 100.0; // local
        return interest;
    }

    public static void main(String[] args) {
        Program15 s = new Program15(10000, 5, 2);
        System.out.println("Bank: " + Program15.bank);
        System.out.println("Simple Interest = " + s.calculateInterest());
    }
}
// Program16.java
// Triangle: check validity and show type (class variable type description)
public class Program16 {
    double a, b, c;
    static String type = "Scalene/Isosceles/Equilateral";

    public Program16(double a, double b, double c) {
        this.a = a; this.b = b; this.c = c;
    }

    public boolean isValid() {
        return (a + b > c) && (a + c > b) && (b + c > a); // local checks
    }

    public String triangleType() {
        if (!isValid()) return "Invalid";
        if (a == b && b == c) return "Equilateral";
        if (a == b || b == c || a == c) return "Isosceles";
        return "Scalene";
    }

    public static void main(String[] args) {
        Program16 t = new Program16(3, 4, 5);
        System.out.println("Type info: " + Program16.type);
        System.out.println("Valid? " + t.isValid());
        System.out.println("Triangle type: " + t.triangleType());
    }
}
// Program17.java
// StudentGrade: assign grade based on marks with passingMarks class variable
public class Program17 {
    String name;
    double marks;
    static double passingMarks = 40.0;

    public Program17(String name, double marks) {
        this.name = name;
        this.marks = marks;
    }

    public String grade() {
        double m = marks; // local
        if (m >= 90) return "A+";
        if (m >= 75) return "A";
        if (m >= 60) return "B";
        if (m >= passingMarks) return "C";
        return "Fail";
    }

    public static void main(String[] args) {
        Program17 s = new Program17("Manoj", 78.5);
        System.out.println(s.name + " Grade = " + s.grade());
    }
}
// Program18.java
// ElectricBill: calculate total bill using units and fixedCharge
public class Program18 {
    double units;
    static double fixedCharge = 50.0;

    public Program18(double units) {
        this.units = units;
    }

    public double calculateBill() {
        // local variable for unit rate (example slab simple)
        double ratePerUnit = 5.0;
        double bill = (units * ratePerUnit) + fixedCharge; // local calc
        return bill;
    }

    public static void main(String[] args) {
        Program18 e = new Program18(120);
        System.out.println("Fixed charge = " + Program18.fixedCharge);
        System.out.println("Total bill = " + e.calculateBill());
    }
}
// Program19.java
// Product: calculate final price using discountRate class variable
public class Program19 {
    int id;
    String name;
    double price;
    static double discountRate = 10.0; // percent

    public Program19(int id, String name, double price) {
        this.id = id; this.name = name; this.price = price;
    }

    public double finalPrice() {
        double discount = (price * discountRate) / 100.0; // local
        return price - discount;
    }

    public static void main(String[] args) {
        Program19 p = new Program19(1, "Notebook", 200.0);
        System.out.println("Product: " + p.name + ", Price = " + p.price);
        System.out.println("Final price after " + Program19.discountRate + "% discount = " + p.finalPrice());
    }
}
// Program20.java
// BankAccount (withdrawal check) with bankCode as class variable
public class Program20 {
    double balance;
    static String bankCode = "BK001";

    public Program20(double balance) {
        this.balance = balance;
    }

    public boolean withdraw(double amount) {
        double fee = 0.0; // local
        if (amount + fee > balance) return false;
        balance -= (amount + fee);
        return true;
    }

    public static void main(String[] args) {
        Program20 acc = new Program20(5000.0);
        double withdrawAmt = 6000.0; // local
        boolean success = acc.withdraw(withdrawAmt);
        System.out.println("Withdrawal of " + withdrawAmt + (success ? " succeeded." : " failed - insufficient balance."));
        System.out.println("Remaining balance = " + acc.balance);
        System.out.println("Bank code = " + Program20.bankCode);
    }
}
// Program21.java
// Palindrome check with static countChecks
import java.util.Scanner;
public class Program21 {
    static int countChecks = 0;

    public boolean isPalindrome(int n) {
        countChecks++;
        int orig = n;
        int rev = 0;
        int temp = Math.abs(n);
        while (temp > 0) {
            rev = rev * 10 + (temp % 10);
            temp /= 10;
        }
        if (n < 0) rev = -rev; // treat negative numbers consistently
        return rev == orig;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int val = sc.nextInt(); // local
        Program21 p = new Program21();
        System.out.println(val + " palindrome? " + p.isPalindrome(val));
        System.out.println("Total checks = " + Program21.countChecks);
        sc.close();
    }
}
// Program22.java
// Armstrong check with static totalChecks (works for any number but uses digit-power matching)
import java.util.Scanner;
public class Program22 {
    static int totalChecks = 0;

    public boolean isArmstrong(int n) {
        totalChecks++;
        int temp = Math.abs(n);
        String s = String.valueOf(temp);
        int power = s.length();
        int sum = 0;
        while (temp > 0) {
            int d = temp % 10;
            int p = 1;
            for (int i = 0; i < power; i++) p *= d;
            sum += p;
            temp /= 10;
        }
        return sum == Math.abs(n);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        Program22 a = new Program22();
        System.out.println(num + " Armstrong? " + a.isArmstrong(num));
        System.out.println("Total checks = " + Program22.totalChecks);
        sc.close();
    }
}
// Program23.java
// PrimeChecker with totalPrimeChecks
import java.util.Scanner;
public class Program23 {
    static int totalPrimeChecks = 0;

    public boolean isPrime(int n) {
        totalPrimeChecks++;
        if (n <= 1) return false;
        if (n <= 3) return true;
        if (n % 2 == 0) return false;
        for (int i = 3; i * i <= n; i += 2)
            if (n % i == 0) return false;
        return true;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        Program23 pc = new Program23();
        System.out.println(n + " prime? " + pc.isPrime(n));
        System.out.println("Total prime checks = " + Program23.totalPrimeChecks);
        sc.close();
    }
}
// Program24.java
// Fibonacci first N terms with seriesCount
import java.util.Scanner;
public class Program24 {
    static int seriesCount = 0;

    public void printFibonacci(int n) {
        seriesCount++;
        int a = 0, b = 1;
        for (int i = 1; i <= n; i++) {
            System.out.print(a + (i==n ? "" : " "));
            int next = a + b;
            a = b; b = next;
        }
        System.out.println();
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(); // local
        Program24 f = new Program24();
        f.printFibonacci(n);
        System.out.println("Series calls = " + Program24.seriesCount);
        sc.close();
    }
}
// Program25.java
// Factorial with totalCalls
import java.util.Scanner;
public class Program25 {
    static int totalCalls = 0;

    public long factorial(int n) {
        totalCalls++;
        long res = 1;
        for (int i = 2; i <= n; i++) res *= i;
        return res;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        Program25 fac = new Program25();
        System.out.println(n + "! = " + fac.factorial(n));
        System.out.println("Total factorial calls = " + Program25.totalCalls);
        sc.close();
    }
}
// Program26.java
// Matrix 2x2 add and subtract using local variables and class variable matrixType
import java.util.Scanner;
public class Program26 {
    static String matrixType = "2x2";

    int[][] A = new int[2][2];
    int[][] B = new int[2][2];

    public int[][] add() {
        int[][] C = new int[2][2];
        for (int i = 0; i < 2; i++)
            for (int j = 0; j < 2; j++)
                C[i][j] = A[i][j] + B[i][j];
        return C;
    }

    public int[][] subtract() {
        int[][] C = new int[2][2];
        for (int i = 0; i < 2; i++)
            for (int j = 0; j < 2; j++)
                C[i][j] = A[i][j] - B[i][j];
        return C;
    }

    public static void printMatrix(int[][] M) {
        for (int i = 0; i < 2; i++) {
            for (int j = 0; j < 2; j++) System.out.print(M[i][j] + " ");
            System.out.println();
        }
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Program26 m = new Program26();
        System.out.println("Enter 4 elements of matrix A:");
        for (int i = 0; i < 2; i++) for (int j = 0; j < 2; j++) m.A[i][j] = sc.nextInt();
        System.out.println("Enter 4 elements of matrix B:");
        for (int i = 0; i < 2; i++) for (int j = 0; j < 2; j++) m.B[i][j] = sc.nextInt();

        System.out.println("Matrix type = " + Program26.matrixType);
        System.out.println("A + B =");
        printMatrix(m.add());
        System.out.println("A - B =");
        printMatrix(m.subtract());
        sc.close();
    }
}
// Program27.java
// StringReverse with totalReversals
import java.util.Scanner;
public class Program27 {
    static int totalReversals = 0;

    public String reverse(String s) {
        totalReversals++;
        char[] arr = s.toCharArray();
        int i = 0, j = arr.length - 1;
        while (i < j) {
            char t = arr[i]; arr[i] = arr[j]; arr[j] = t;
            i++; j--;
        }
        return new String(arr);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String line = sc.nextLine();
        Program27 r = new Program27();
        System.out.println("Reversed: " + r.reverse(line));
        System.out.println("Total reversals = " + Program27.totalReversals);
        sc.close();
    }
}
// Program28.java
// WordCounter with language class variable
import java.util.Scanner;
public class Program28 {
    static String language = "English";

    public int countWords(String sentence) {
        if (sentence == null || sentence.trim().isEmpty()) return 0;
        String[] parts = sentence.trim().split("\\s+"); // local split
        return parts.length;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String sentence = sc.nextLine();
        Program28 wc = new Program28();
        System.out.println("Language = " + Program28.language);
        System.out.println("Word count = " + wc.countWords(sentence));
        sc.close();
    }
}
