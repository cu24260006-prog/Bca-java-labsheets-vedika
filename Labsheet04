// Program01.java
// Student class with instance fields name, age and class variable count
public class Program01 {
    String name;
    int age;
    static int count = 0; // class variable to track number of students

    public Program01(String name, int age) {
        this.name = name;
        this.age = age;
        count++;
    }

    public void display() {
        System.out.println("Name: " + name + ", Age: " + age);
    }

    public static void main(String[] args) {
        // local variables used to assign values
        String n1 = "Asha";
        int a1 = 19;
        Program01 s1 = new Program01(n1, a1);

        String n2 = "Ravi";
        int a2 = 20;
        Program01 s2 = new Program01(n2, a2);

        s1.display();
        s2.display();

        System.out.println("Total students = " + Program01.count);
    }
}

// Program02.java
// Rectangle class with instance fields length, breadth and class variable shapeName
public class Program02 {
    double length;
    double breadth;
    static String shapeName = "Rectangle";

    public Program02(double length, double breadth) {
        this.length = length;
        this.breadth = breadth;
    }

    public double area() {
        // local variable to compute area
        double area = length * breadth;
        return area;
    }

    public static void main(String[] args) {
        // local variables for input values
        double l = 5.0;
        double b = 3.0;
        Program02 r = new Program02(l, b);

        System.out.println("Shape: " + Program02.shapeName);
        System.out.println("Length = " + l + ", Breadth = " + b);
        System.out.println("Area = " + r.area());
    }
}
// Program03.java
// Circle class with instance field radius and class variable PI
public class Program03 {
    double radius;
    static final double PI = 3.14;

    public Program03(double radius) {
        this.radius = radius;
    }

    public double area() {
        double a = PI * radius * radius; // local variable a
        return a;
    }

    public double circumference() {
        double c = 2 * PI * radius; // local variable c
        return c;
    }

    public static void main(String[] args) {
        double rVal = 2.5;
        Program03 c = new Program03(rVal);

        System.out.println("Radius = " + rVal);
        System.out.println("Area = " + c.area());
        System.out.println("Circumference = " + c.circumference());
    }
}
