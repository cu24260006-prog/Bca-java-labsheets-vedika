// Program30.java
// Library class: update issue and return operations using local variables and static libraryName
import java.util.Scanner;
public class Program30 {
    static String libraryName = "City Library";
    int booksAvailable;

    public Program30(int booksAvailable) {
        this.booksAvailable = booksAvailable;
    }

    public boolean issueBook(int count) {
        // local variable count
        if (count <= 0) return false;
        if (booksAvailable >= count) {
            booksAvailable -= count;
            return true;
        }
        return false;
    }

    public void returnBook(int count) {
        if (count > 0) booksAvailable += count;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int initial = sc.nextInt(); // initial books
        Program30 lib = new Program30(initial);

        int toIssue = sc.nextInt(); // local
        boolean ok = lib.issueBook(toIssue);
        System.out.println("Issue " + toIssue + (ok ? " succeeded." : " failed."));
        int toReturn = sc.nextInt(); // local
        lib.returnBook(toReturn);
        System.out.println("After operations, books available = " + lib.booksAvailable);
        System.out.println("Library = " + Program30.libraryName);
        sc.close();
    }
}

