    public Program21(String title, String author) {
        this(title, author, 500.0);
    }

    // Constructor 3: default (Unknown)
    public Program21() {
        this("Unknown", "Unknown", 0.0);
    }

    public void printDetails() {

// Program22.java
// Rectangle class showing constructor overloading (no-arg, one-arg, two-arg)
public class Program22 {
    double length;
    double breadth;

    // No-arg constructor -> default 1x1
    public Program22() {
        this.length = 1.0; this.breadth = 1.0;
    }

    // One-arg -> square
    public Program22(double side) {
        this.length = side; this.breadth = side;
    }

    // Two-arg -> rectangle
    public Program22(double length, double breadth) {
        this.length = length; this.breadth = breadth;
    }

    public double area() {
        double a = length * breadth;
        return a;
    }

    public static void main(String[] args) {
        Program22 r1 = new Program22();
        Program22 r2 = new Program22(4.0);       // square 4x4
        Program22 r3 = new Program22(5.0, 3.0);  // rectangle

        System.out.println("r1 area = " + r1.area());
        System.out.println("r2 area = " + r2.area());
        System.out.println("r3 area = " + r3.area());
    }
}
// Program23.java
// Employee with overloaded constructors (all fields, id+name)
public class Program23 {
    int id;
    String name;
    double salary;

    // All fields
    public Program23(int id, String name, double salary) {
        this.id = id; this.name = name; this.salary = salary;
    }

    // id and name only -> default salary = 30000
    public Program23(int id, String name) {
        this(id, name, 30000.0);
    }

    public void display() {
        System.out.println("ID: " + id + " | Name: " + name + " | Salary: " + salary);
    }

    public static void main(String[] args) {
        Program23 e1 = new Program23(101, "Amit", 45000.0);
        Program23 e2 = new Program23(102, "Neha"); // salary 30000
        e1.display();
        e2.display();
    }
}
// Program24.java
// Car class with overloaded constructors (brand only, brand+model, brand+model+year)
public class Program24 {
    String brand;
    String model;
    int year;

    public Program24(String brand) {
        this(brand, "Unknown", 0);
    }

    public Program24(String brand, String model) {
        this(brand, model, 0);
    }

    public Program24(String brand, String model, int year) {
        this.brand = brand; this.model = model; this.year = year;
    }

    public void display() {
        System.out.println("Brand: " + brand + " | Model: " + model + " | Year: " + year);
    }

    public static void main(String[] args) {
        Program24 c1 = new Program24("Maruti");
        Program24 c2 = new Program24("Honda", "City");
        Program24 c3 = new Program24("Toyota", "Corolla", 2019);

        c1.display();
        c2.display();
        c3.display();
    }
}
// Program25.java
// Explanation of why constructor overloading is needed + simple example printed
public class Program25 {
    public static void main(String[] args) {
        System.out.println("Why constructor overloading?");
        System.out.println("- It allows creating objects with different sets of initial data.");
        System.out.println("- Provides convenience and readability (default, partial or full initialization).");
        System.out.println("- Example: a Book can be created with full details, just title+author, or default values.");

        // small runtime example (reuse Program21 style)
        Program21 example1 = new Program21("DSA", "Reddy", 600.0);
        Program21 example2 = new Program21("Networking", "Khan");
        Program21 example3 = new Program21();

        example1.printDetails();
        example2.printDetails();
        example3.printDetails();
    }
}

// small helper class used above (same as Program21) to demonstrate
class Program21 {
    String title; String author; double price;
    public Program21(String t, String a, double p) { title=t; author=a; price=p;}
    public Program21(String t, String a) { this(t,a,500.0); }
    public Program21() { this("Unknown","Unknown",0.0); }
    public void printDetails(){ System.out.println(title + " | " + author + " | " + price); }
}
// Program26.java
// Calculator class demonstrating method overloading add()
public class Program26 {
    // add two ints
    public int add(int a, int b) {
        int res = a + b; // local
        return res;
    }

    // add two doubles
    public double add(double a, double b) {
        double res = a + b;
        return res;
    }

    // concatenate two strings
    public String add(String a, String b) {
        String res = a + b;
        return res;
    }

    public static void main(String[] args) {
        Program26 calc = new Program26();
        System.out.println("int add: " + calc.add(5, 7));
        System.out.println("double add: " + calc.add(3.2, 4.5));
        System.out.println("String add: " + calc.add("Hello ", "World"));
    }
}
// Program27.java
// Printer class with overloaded print methods for int, double and string
public class Program27 {
    public void print(int x) {
        System.out.println("Int: " + x);
    }

    public void print(double x) {
        System.out.println("Double: " + x);
    }

    public void print(String s) {
        System.out.println("String: " + s);
    }

    public static void main(String[] args) {
        Program27 p = new Program27();
        p.print(100);
        p.print(3.1415);
        p.print("Print this line");
    }
}
// Program28.java
// MathOperations with overloaded multiply(): 2 ints, 3 ints, 2 doubles
public class Program28 {
    public int multiply(int a, int b) {
        return a * b;
    }

    public int multiply(int a, int b, int c) {
        return a * b * c;
    }

    public double multiply(double a, double b) {
        return a * b;
    }

    public static void main(String[] args) {
        Program28 mo = new Program28();
        System.out.println("2 ints: " + mo.multiply(2,3));
        System.out.println("3 ints: " + mo.multiply(2,3,4));
        System.out.println("2 doubles: " + mo.multiply(2.5, 4.0));
    }
}
// Program29.java
// Short code examples showing method overloading vs method overriding

// Base class demonstrating overloading
class OverloadExample {
    public void show() { System.out.println("show() with no args"); }
    public void show(int x) { System.out.println("show(int): " + x); } // overloaded method
}

// Base and derived class demonstrating overriding
class Base {
    public void greet() { System.out.println("Hello from Base"); }
}
class Derived extends Base {
    @Override
    public void greet() { System.out.println("Hello from Derived (overridden)"); }
}

public class Program29 {
    public static void main(String[] args) {
        System.out.println("Method Overloading example:");
        OverloadExample o = new OverloadExample();
        o.show();
        o.show(10);

        System.out.println("\nMethod Overriding example:");
        Base b = new Base();
        b.greet();
        Base d = new Derived(); // runtime polymorphism
        d.greet();
    }
}
// Program30.java
// ShapeArea with overloaded area() methods: square, rectangle, circle
public class Program30 {
    // square area
    public int area(int side) {
        return side * side;
    }

    // rectangle area
    public int area(int length, int breadth) {
        return length * breadth;
    }

    // circle area (double)
    public double area(double radius) {
        return Math.PI * radius * radius;
    }

    public static void main(String[] args) {
        Program30 s = new Program30();
        System.out.println("Square area (side=5): " + s.area(5));
        System.out.println("Rectangle area (5x3): " + s.area(5,3));
        System.out.println("Circle area (r=2.5): " + s.area(2.5));
    }
}



