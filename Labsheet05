// Program19.java
// Game with gameName, genre, static platform, method uses local rating
import java.util.Scanner;
public class Program19 {
    String gameName;
    String genre;
    static String platform = "PC";

    public Program19(String gameName, String genre) {
        this.gameName = gameName;
        this.genre = genre;
    }

    public void showDetails(double rating) {
        double r = rating; // local
        System.out.println("Game: " + gameName + " | Genre: " + genre + " | Platform: " + platform + " | Rating: " + r);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Game name: ");
        String name = sc.nextLine();
        System.out.print("Genre: ");
        String g = sc.nextLine();
        System.out.print("Rating (0-10): ");
        double rate = sc.nextDouble();

        Program19 game = new Program19(name, g);
        game.showDetails(rate);

        sc.close();
    }
}
// Program20.java
// Store with itemName, static storeLocation, method uses local discount to show final price
import java.util.Scanner;
public class Program20 {
    String itemName;
    static String storeLocation = "Downtown";

    public Program20(String itemName) {
        this.itemName = itemName;
    }

    public double finalPrice(double price, double discountPercent) {
        double discount = discountPercent; // local
        double finalP = price - (price * discount / 100.0);
        return finalP;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Item name: ");
        String item = sc.nextLine();
        System.out.print("Price: ");
        double price = sc.nextDouble();
        System.out.print("Discount %: ");
        double disc = sc.nextDouble();

        Program20 s = new Program20(item);
        double fp = s.finalPrice(price, disc);
        System.out.println("Store location: " + Program20.storeLocation);
        System.out.println("Final price for " + s.itemName + " = " + fp);

        sc.close();
    }
}
