// Program10.java
// BankAccount with accountNumber, balance, constructor and deposit method
public class Program10 {
    String accountNumber;
    double balance;

    public Program10(String accountNumber, double balance) {
        this.accountNumber = accountNumber;
        this.balance = balance;
    }

    public void deposit(double amount) {
        double amt = amount; // local variable
        if (amt > 0) {
            balance += amt;
            System.out.println("Deposited: Rs " + amt + " | New Balance: Rs " + balance);
        } else {
            System.out.println("Invalid deposit amount");
        }
    }

    public static void main(String[] args) {
        Program10 acc = new Program10("AC12345", 2000.0);
        acc.deposit(1500.0);
    }
}
// Program11.java
// Account with static bankName, deposit method using local variable amount
import java.util.Scanner;
public class Program11 {
    static String bankName = "MyBank";
    String accountNumber;
    double balance;

    public Program11(String accountNumber, double balance) {
        this.accountNumber = accountNumber;
        this.balance = balance;
    }

    public void deposit(double amt) {
        double amount = amt; // local variable
        if (amount > 0) {
            balance += amount;
            System.out.println("Deposited: " + amount + " | New balance: " + balance);
        } else {
            System.out.println("Invalid deposit amount");
        }
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Account no: ");
        String acc = sc.next();
        System.out.print("Initial balance: ");
        double bal = sc.nextDouble();

        Program11 ac = new Program11(acc, bal);
        System.out.println("Bank: " + Program11.bankName);

        System.out.print("Enter deposit amount: ");
        double d = sc.nextDouble();
        ac.deposit(d);

        sc.close();
    }
}
// Program12.java
// Course with courseName, duration and static totalCourses
import java.util.Scanner;
public class Program12 {
    String courseName;
    int duration; // in weeks/months
    static int totalCourses = 0;

    public Program12(String courseName, int duration) {
        this.courseName = courseName;
        this.duration = duration;
        totalCourses++;
    }

    public void display() {
        System.out.println("Course: " + courseName + " | Duration: " + duration);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Course name: ");
        String name = sc.nextLine();
        System.out.print("Duration (weeks): ");
        int dur = sc.nextInt();

        Program12 c = new Program12(name, dur);
        c.display();
        System.out.println("Total courses = " + Program12.totalCourses);

        sc.close();
    }
}
// Program13.java
// Inventory with itemName, quantity and static totalItems
import java.util.Scanner;
public class Program13 {
    String itemName;
    int quantity;
    static int totalItems = 0;

    public Program13(String itemName, int quantity) {
        this.itemName = itemName;
        this.quantity = quantity;
        totalItems += quantity;
    }

    public void display() {
        System.out.println("Item: " + itemName + " | Quantity: " + quantity);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("Item name: ");
        String item = sc.nextLine();
        System.out.print("Quantity: ");
        int q = sc.nextInt();

        Program13 inv = new Program13(item, q);
        inv.display();
        System.out.println("Total items in inventory = " + Program13.totalItems);

        sc.close();
    }
}
// Program14.java
// City with cityName and static countryName; method uses local variable population
import java.util.Scanner;
public class Program14 {
    String cityName;
    static String countryName = "India";

    public Program14(String cityName) {
        this.cityName = cityName;
    }

    public void showDetails(int population) {
        int pop = population; // local variable
        System.out.println("City: " + cityName + ", Population: " + pop + ", Country: " + countryName);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("City name: ");
        String c = sc.nextLine();
        System.out.print("Population (integer): ");
        int p = sc.nextInt();

        Program14 city = new Program14(c);
        city.showDetails(p);

        sc.close();
    }
}
// Program15.java
// LaptopStore with laptopName, price and static storeName, method applies discount using local variable
import java.util.Scanner;
public class Program15 {
    String laptopName;
    double price;
    static String storeName = "LaptopWorld";

    public Program15(String laptopName, double price) {
        this.laptopName = laptopName;
        this.price = price;
    }

    public double applyDiscount(double discountPercent) {
        double discount = discountPercent; // local
        double finalPrice = price - (price * discount / 100.0);
        return finalPrice;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Laptop name: ");
        String name = sc.nextLine();
        System.out.print("Price: ");
        double pr = sc.nextDouble();
        System.out.print("Discount %: ");
        double dis = sc.nextDouble();

        Program15 shop = new Program15(name, pr);
        System.out.println("Store: " + Program15.storeName);
        System.out.println("Final price = " + shop.applyDiscount(dis));

        sc.close();
    }
}
// Program16.java
// Hotel class with roomType and static hotelName, method to display booking details
import java.util.Scanner;
public class Program16 {
    String roomType;
    static String hotelName = "Comfort Inn";

    public Program16(String roomType) {
        this.roomType = roomType;
    }

    public void bookingDetails(int nights, double ratePerNight) {
        int n = nights; // local
        double total = n * ratePerNight; // local
        System.out.println("Hotel: " + hotelName + " | Room: " + roomType + " | Nights: " + n + " | Total = " + total);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Room type: ");
        String rt = sc.nextLine();
        System.out.print("Nights: ");
        int nights = sc.nextInt();
        System.out.print("Rate per night: ");
        double rate = sc.nextDouble();

        Program16 h = new Program16(rt);
        h.bookingDetails(nights, rate);

        sc.close();
    }
}
