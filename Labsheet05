// Program01.java
// Student class with name and age, constructor and display method
public class Program01 {
    String name;
    int age;

    public Program01(String name, int age) {
        this.name = name;
        this.age = age;
    }

    public void display() {
        System.out.println("Name: " + name + ", Age: " + age);
    }

    public static void main(String[] args) {
        // local variables used to create objects
        Program01 s1 = new Program01("Asha", 19);
        Program01 s2 = new Program01("Rohit", 20);

        s1.display();
        s2.display();
    }
}
// Program02.java
// Book class with title, static countBooks and print method
public class Program02 {
    String title;
    static int countBooks = 0;

    public Program02(String title) {
        this.title = title;
        countBooks++;
    }

    public void printDetails() {
        System.out.println("Title: " + title);
    }

    public static void main(String[] args) {
        Program02 b1 = new Program02("Data Structures");
        Program02 b2 = new Program02("Java Fundamentals");

        b1.printDetails();
        b2.printDetails();
        System.out.println("Total books created = " + Program02.countBooks);
    }
}
// Program03.java
// Circle class with radius, constructor and method to calculate area
public class Program03 {
    double radius;

    public Program03(double radius) {
        this.radius = radius;
    }

    public double area() {
        double area = Math.PI * radius * radius; // local variable area
        return area;
    }

    public static void main(String[] args) {
        Program03 c = new Program03(2.5);
        System.out.println("Radius = " + c.radius);
        System.out.println("Area = " + c.area());
    }
}
// Program04.java
// Counter class with static count incremented in constructor
public class Program04 {
    static int count = 0;

    public Program04() {
        count++;
    }

    public void displayCount() {
        System.out.println("Current count = " + count);
    }

    public static void main(String[] args) {
        Program04 x1 = new Program04();
        Program04 x2 = new Program04();
        Program04 x3 = new Program04();

        x3.displayCount(); // shows 3
    }
}
// Program05.java
// Employee class with empName and salary and method using local variables to format output
public class Program05 {
    String empName;
    double salary;

    public Program05(String empName, double salary) {
        this.empName = empName;
        this.salary = salary;
    }

    public void display() {
        String out = "Employee: " + empName + " | Salary: Rs " + String.format("%.2f", salary); // local formatting
        System.out.println(out);
    }

    public static void main(String[] args) {
        Program05 e = new Program05("Sunil", 45000.0);
        e.display();
    }
}
// Program06.java
// Rectangle class with length and width, constructor and area method
public class Program06 {
    double length;
    double width;

    public Program06(double length, double width) {
        this.length = length;
        this.width = width;
    }

    public double area() {
        double a = length * width; // local
        return a;
    }

    public static void main(String[] args) {
        Program06 r = new Program06(5.0, 3.0);
        System.out.println("Length = " + r.length + ", Width = " + r.width);
        System.out.println("Area = " + r.area());
    }
}
// Program07.java
// Car class with model, year and static totalCars incremented in constructor
public class Program07 {
    String model;
    int year;
    static int totalCars = 0;

    public Program07(String model, int year) {
        this.model = model;
        this.year = year;
        totalCars++;
    }

    public void display() {
        System.out.println("Model: " + model + ", Year: " + year);
    }

    public static void main(String[] args) {
        Program07 c1 = new Program07("Swift", 2020);
        Program07 c2 = new Program07("Tiago", 2021);

        c1.display();
        c2.display();
        System.out.println("Total cars = " + Program07.totalCars);
    }
}
// Program08.java
// Box class with height, width, depth, constructor and method to calculate volume
public class Program08 {
    double height;
    double width;
    double depth;

    public Program08(double height, double width, double depth) {
        this.height = height;
        this.width = width;
        this.depth = depth;
    }

    public double volume() {
        double vol = height * width * depth; // local
        return vol;
    }

    public static void main(String[] args) {
        Program08 box = new Program08(2.0, 3.0, 4.0);
        System.out.println("Volume = " + box.volume());
    }
}
