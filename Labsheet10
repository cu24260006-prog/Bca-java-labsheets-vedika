public class Program02 {
    public static void main(String[] args) {
        ArrayList<String> list = new ArrayList<>();
        list.add("apple");
        list.add("banana");
        list.add("cherry");

        System.out.println("Elements: " + list);
        System.out.println("Size = " + list.size());
    }
}
// Program03.java
// Insert an element at a specific index in an ArrayList
import java.util.ArrayList;

public class Program03 {
    public static void main(String[] args) {
        ArrayList<String> list = new ArrayList<>();
        list.add("one");
        list.add("two");
        list.add("four");

        System.out.println("Before insert: " + list);
        list.add(2, "three"); // insert at index 2
        System.out.println("After insert at index 2: " + list);
    }
}
// Program04.java
// Remove an element from an ArrayList by index and by value
import java.util.ArrayList;

public class Program04 {
    public static void main(String[] args) {
        ArrayList<String> list = new ArrayList<>();
        list.add("red");
        list.add("green");
        list.add("blue");
        list.add("green");

        System.out.println("Original list: " + list);

        // remove by index
        String removed = list.remove(1); // removes element at index 1 ("green")
        System.out.println("Removed by index 1 -> " + removed);
        System.out.println("After removing index: " + list);

        // remove by value (first occurrence)
        boolean ok = list.remove("green");
        System.out.println("Removed by value 'green'? " + ok);
        System.out.println("After removing value: " + list);
    }
}
// Program05.java
// Update an element in an ArrayList at a given index
import java.util.ArrayList;

public class Program05 {
    public static void main(String[] args) {
        ArrayList<Integer> list = new ArrayList<>();
        list.add(100);
        list.add(200);
        list.add(300);

        System.out.println("Before update: " + list);
        list.set(1, 250); // update index 1
        System.out.println("After update at index 1: " + list);
    }
}
// Program06.java
// Iterate through an ArrayList using a for-each loop
import java.util.ArrayList;

public class Program06 {
    public static void main(String[] args) {
        ArrayList<String> names = new ArrayList<>();
        names.add("Asha");
        names.add("Rohit");
        names.add("Meera");

        System.out.println("Iterating with for-each:");
        for (String name : names) {
            System.out.println(name);
        }
    }
}
// Program07.java
// Iterate through an ArrayList using an Iterator
import java.util.ArrayList;
import java.util.Iterator;

public class Program07 {
    public static void main(String[] args) {
        ArrayList<Integer> numbers = new ArrayList<>();
        for (int i = 1; i <= 5; i++) numbers.add(i);

        System.out.println("Iterating with Iterator:");
        Iterator<Integer> it = numbers.iterator();
        while (it.hasNext()) {
            Integer v = it.next();
            System.out.println(v);
        }
    }
}
// Program08.java
// Sort an ArrayList of integers in ascending order
import java.util.ArrayList;
import java.util.Collections;

public class Program08 {
    public static void main(String[] args) {
        ArrayList<Integer> list = new ArrayList<>();
        list.add(42);
        list.add(7);
        list.add(19);
        list.add(3);

        System.out.println("Before sort: " + list);
        Collections.sort(list);
        System.out.println("After ascending sort: " + list);
    }
}
