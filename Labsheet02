// Program01.java
// Area of a triangle using Heron's formula
import java.util.Scanner;
public class Program01 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        // input three sides
        double a = sc.nextDouble();
        double b = sc.nextDouble();
        double c = sc.nextDouble();

        double s = (a + b + c) / 2.0;
        // check valid triangle first
        if (a + b > c && a + c > b && b + c > a) {
            double area = Math.sqrt(s * (s - a) * (s - b) * (s - c));
            System.out.println("Area = " + area);
        } else {
            System.out.println("Not a valid triangle");
        }
        sc.close();
    }
}
// Program02.java
// Calculate compound interest: A = P * (1 + r/100)^t
import java.util.Scanner;
public class Program02 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double principal = sc.nextDouble(); // P
        double rate = sc.nextDouble();      // annual rate in percent
        double time = sc.nextDouble();      // time in years (can be fractional)

        double amount = principal * Math.pow(1 + rate / 100.0, time);
        double compoundInterest = amount - principal;

        System.out.println("Amount = " + amount);
        System.out.println("Compound Interest = " + compoundInterest);
        sc.close();
    }
}
// Program03.java
// Distance between (x1,y1) and (x2,y2)
import java.util.Scanner;
public class Program03 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double x1 = sc.nextDouble();
        double y1 = sc.nextDouble();
        double x2 = sc.nextDouble();
        double y2 = sc.nextDouble();

        double dx = x2 - x1;
        double dy = y2 - y1;
        double dist = Math.sqrt(dx * dx + dy * dy);

        System.out.println("Distance = " + dist);
        sc.close();
    }
}
// Program04.java
// Demonstrate prefix and postfix increment using a visitor counter
import java.util.Scanner;
public class Program04 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int visitors = sc.nextInt(); // initial count

        // someone enters (prefix)
        System.out.println("Before prefix increment: " + visitors);
        System.out.println("After prefix ++visitors => " + (++visitors));
        System.out.println("Current visitors: " + visitors);

        // someone leaves and enters (postfix)
        System.out.println("Use postfix visitors++ in expression: " + (visitors++));
        System.out.println("After postfix visitors++ current visitors: " + visitors);

        sc.close();
    }
}
