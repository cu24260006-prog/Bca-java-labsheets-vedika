// Program06.java
// Reduce a number by half using '/=' until it becomes < 1 and count steps
import java.util.Scanner;
public class Program06 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double value = sc.nextDouble();
        int steps = 0;
        while (value >= 1.0) {
            value /= 2.0;
            steps++;
        }
        System.out.println("Steps = " + steps);
        System.out.println("Final value < 1: " + value);
        sc.close();
    }
}
// Program07.java
// Read rainfall for 7 days, accumulate using += and print total
import java.util.Scanner;
public class Program07 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double total = 0.0;
        for (int day = 1; day <= 7; day++) {
            double rain = sc.nextDouble(); // rainfall of that day
            total += rain;
        }
        System.out.println("Total rainfall = " + total);
        sc.close();
    }
}// Program08.java
// Check whether three angles form a valid triangle (sum == 180 and each > 0)
import java.util.Scanner;
public class Program08 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();

        if (a > 0 && b > 0 && c > 0 && a + b + c == 180)
            System.out.println("Valid triangle");
        else
            System.out.println("Invalid triangle");

        sc.close();
    }
}// Program09.java
// Compare two strings lexicographically by comparing characters
import java.util.Scanner;
public class Program09 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s1 = sc.next(); // no spaces expected; use nextLine if spaces allowed
        String s2 = sc.next();

        int i = 0;
        int n1 = s1.length();
        int n2 = s2.length();
        int result = 0; // 0 => equal so far, -1 => s1 < s2, +1 => s1 > s2

        while (i < n1 && i < n2) {
            char c1 = s1.charAt(i);
            char c2 = s2.charAt(i);
            if (c1 < c2) { result = -1; break; }
            else if (c1 > c2) { result = 1; break; }
            i++;
        }
        if (result == 0) { // same prefix
            if (n1 < n2) result = -1;
            else if (n1 > n2) result = 1;
            else result = 0;
        }

        if (result < 0) System.out.println(s1 + " comes before " + s2);
        else if (result > 0) System.out.println(s1 + " comes after " + s2);
        else System.out.println(s1 + " is equal to " + s2);

        sc.close();
    }
}
// Program10.java
// Check if a year is leap AND within a given inclusive range
import java.util.Scanner;
public class Program10 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int year = sc.nextInt();
        int start = sc.nextInt(); // range start
        int end = sc.nextInt();   // range end

        boolean isLeap = (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);
        boolean inRange = (year >= start && year <= end);

        if (isLeap && inRange)
            System.out.println("Year is leap and within range");
        else
            System.out.println("Either not leap or not in range");

        sc.close();
    }
}
// Program11.java
// Check student pass condition using logical operators
import java.util.Scanner;
public class Program11 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        double theory = sc.nextDouble();
        double practical = sc.nextDouble();
        double overall = sc.nextDouble();

        boolean pass =
            (theory >= 40 && practical >= 50) || (overall >= 50);

        if (pass)
            System.out.println("Pass");
        else
            System.out.println("Fail");

        sc.close();
    }
}
// Program12.java
// Find smallest of four numbers using nested ternary operators
import java.util.Scanner;
public class Program12 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int d = sc.nextInt();

        int smallest =
            (a < b ?
                (a < c ?
                    (a < d ? a : d) :
                    (c < d ? c : d)) :
                (b < c ?
                    (b < d ? b : d) :
                    (c < d ? c : d)));

        System.out.println("Smallest = " + smallest);

        sc.close();
    }
}
// Program13.java
// Check if character is vowel, consonant, digit, or special symbol using ternary
import java.util.Scanner;
public class Program13 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        char ch = sc.next().charAt(0);

        String result =
            (Character.isDigit(ch)) ? "Digit" :
            ((Character.isLetter(ch)) ?
                ("aeiouAEIOU".indexOf(ch) != -1 ? "Vowel" : "Consonant")
                : "Special Symbol");

        System.out.println(result);

        sc.close();
    }
}
// Program14.java
// Swap two integers without using a temporary variable (XOR)
import java.util.Scanner;
public class Program14 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int a = sc.nextInt();
        int b = sc.nextInt();

        a = a ^ b;
        b = a ^ b;
        a = a ^ b;

        System.out.println("After swap: a = " + a + ", b = " + b);
        sc.close();
    }
}
// Program15.java
// Count number of set bits (1s) in binary representation
import java.util.Scanner;
public class Program15 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int count = 0;

        while (n > 0) {
            count += (n & 1); // add last bit
            n >>= 1;          // right shift
        }

        System.out.println("Set bits = " + count);
        sc.close();
    }
}
// Program16.java
// Multiply and divide by powers of 2 using shift operators
import java.util.Scanner;
public class Program16 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int power = sc.nextInt(); // power of 2

        int multiplied = n << power; // n * 2^power
        int divided = n >> power;    // n / 2^power

        System.out.println("Multiply by 2^" + power + " = " + multiplied);
        System.out.println("Divide by 2^" + power + " = " + divided);

        sc.close();
    }
}
// Program17.java
// Cyclic left rotate bits of an integer by 2 positions
import java.util.Scanner;
public class Program17 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        int rotated = (n << 2) | (n >>> (32 - 2)); // unsigned shift

        System.out.println("After left rotate by 2 = " + rotated);

        sc.close();
    }
}
// Program18.java
// Print first 20 Fibonacci numbers
public class Program18 {
    public static void main(String[] args) {
        int a = 0, b = 1;

        for (int i = 1; i <= 20; i++) {
            System.out.print(a + " ");
            int next = a + b;
            a = b;
            b = next;
        }
    }
}
// Program19.java
// Print all Armstrong numbers between 1 and 1000
public class Program19 {
    public static void main(String[] args) {
        for (int n = 1; n <= 1000; n++) {
            int sum = 0, temp = n;

            while (temp > 0) {
                int digit = temp % 10;
                sum += digit * digit * digit;
                temp /= 10;
            }

            if (sum == n)
                System.out.println(n);
        }
    }
}
// Program20.java
// Accept password until correct one is entered
import java.util.Scanner;
public class Program20 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        final String correct = "java123";
        String input;

        do {
            System.out.print("Enter password: ");
            input = sc.next();
        } while (!input.equals(correct));

        System.out.println("Access Granted!");
        sc.close();
    }
}
// Program21.java
// Display all factors of a number using do-while loop
import java.util.Scanner;
public class Program21 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int i = 1;

        do {
            if (n % i == 0)
                System.out.println(i);
            i++;
        } while (i <= n);

        sc.close();
    }
}
// Program22.java
// Reverse digits of an integer using while loop
import java.util.Scanner;
public class Program22 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int rev = 0;

        while (n > 0) {
            rev = rev * 10 + (n % 10);
            n /= 10;
        }

        System.out.println("Reversed = " + rev);
        sc.close();
    }
}// Program23.java
// Check palindrome using while loop
import java.util.Scanner;
public class Program23 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int original = n;
        int rev = 0;

        while (n > 0) {
            rev = rev * 10 + n % 10;
            n /= 10;
        }

        if (rev == original)
            System.out.println("Palindrome");
        else
            System.out.println("Not Palindrome");

        sc.close();
    }
}
// Program24.java
// Find max and min in array using for-each loop
import java.util.Scanner;
public class Program24 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int arr[] = new int[n];

        for (int i = 0; i < n; i++)
            arr[i] = sc.nextInt();

        int max = arr[0];
        int min = arr[0];

        for (int x : arr) {
            if (x > max) max = x;
            if (x < min) min = x;
        }

        System.out.println("Max = " + max);
        System.out.println("Min = " + min);

        sc.close();
    }
}
// Program25.java
// Calculate average marks of students in 2D array using for-each loop
import java.util.Scanner;
public class Program25 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int rows = sc.nextInt();
        int cols = sc.nextInt();
        int arr[][] = new int[rows][cols];

        for (int i = 0; i < rows; i++)
            for (int j = 0; j < cols; j++)
                arr[i][j] = sc.nextInt();

        int sum = 0;
        int count = 0;

        for (int[] row : arr)
            for (int val : row) {
                sum += val;
                count++;
            }

        double avg = sum / (double) count;

        System.out.println("Average = " + avg);

        sc.close();
    }
}
// Program26.java
// Display weekday or weekend based on day number (1–7)
import java.util.Scanner;
public class Program26 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int day = sc.nextInt();
        switch (day) {
            case 1: case 2: case 3: case 4: case 5:
                System.out.println("Weekday");
                break;
            case 6: case 7:
                System.out.println("Weekend");
                break;
            default:
                System.out.println("Invalid day");
        }

        sc.close();
    }
}
// Program27.java
// Print meaning of grade using switch-case
import java.util.Scanner;
public class Program27 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        char grade = Character.toUpperCase(sc.next().charAt(0));

        switch (grade) {
            case 'A': System.out.println("Excellent"); break;
            case 'B': System.out.println("Good"); break;
            case 'C': System.out.println("Average"); break;
            case 'D': System.out.println("Below Average"); break;
            case 'E': System.out.println("Poor"); break;
            case 'F': System.out.println("Fail"); break;
            default: System.out.println("Invalid Grade");
        }

        sc.close();
    }
}
// Program28.java
// Print numbers 1–50 skipping perfect squares
public class Program28 {
    public static void main(String[] args) {
        for (int i = 1; i <= 50; i++) {
            int sq = (int) Math.sqrt(i);
            if (sq * sq == i)
                continue; // skip perfect squares
            System.out.println(i);
        }
    }
}
// Program29.java
// Generate random integers (1–100) and stop when divisible by both 7 and 13
public class Program29 {
    public static void main(String[] args) {
        java.util.Random r = new java.util.Random();

        while (true) {
            int n = r.nextInt(100) + 1; // 1 to 100
            System.out.println(n);

            if (n % 7 == 0 && n % 13 == 0) {
                System.out.println("Found number divisible by 7 and 13!");
                break;
            }
        }
    }
}
// Program30.java
// Power of 4 check, toggle 3rd bit, print table with continue & break
import java.util.Scanner;
public class Program30 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        // 1. Check if power of 4 using bit operations
        boolean isPowerOf4 = (n > 0) && ((n & (n - 1)) == 0) && ((n & 0x55555555) == n);
        System.out.println("Power of 4? " + isPowerOf4);

        // 2. Toggle 3rd bit (bit index 2)
        int toggled = n ^ (1 << 2);
        System.out.println("After toggling 3rd bit: " + toggled);

        // 3. Print multiplication table; skip multiples of 6; break on multiple of 48
        for (int i = 1; i <= 20; i++) {
            int value = n * i;

            if (value % 48 == 0) {
                System.out.println("Reached multiple of 48. Stopping.");
                break;
            }

            if (value % 6 == 0) {
                continue; // skip multiples of 6
            }

            System.out.println(n + " x " + i + " = " + value);
        }

        sc.close();
    }
}







